{
    "name": "Canvas Tab Page",
    "description": "Provides access to four common websites and includes integration with the Canvas platform for assignments, courses, and more.",
    "version": "1.0",
    "manifest_version": 2,
    "icons": { 
        "16": "assets/icons/logo.png",
        "48": "assets/icons/logo.png",
        "128": "assets/icons/logo.png" 
    },
   "permissions" : [
    "https://myuni.adelaide.edu.au/*",
    "http://myuni.adelaide.edu.au/*",
    "https://canvas.instructure.com/*",
    "http://canvas.instructure.com/*",
    "http://robbiesymonds.me/*",
    "tabs",
    "cookies",
    "management",
    "storage",
    "notifications"
  ],
    "chrome_url_overrides" : {
        "newtab": "index.html"
    }
 }